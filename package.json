{
  "name": "vue-inter",
  "version": "2.0.1",
  "description": "Simple i18n for Vue.js",
  "repository": {
    "url": "egoist/vue-inter",
    "type": "git"
  },
  "main": "dist/vue-inter.cjs.js",
  "unpkg": "dist/vue-inter.min.js",
  "cdn": "dist/vue-inter.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run lint && tyu",
    "lint": "xo",
    "prepublishOnly": "npm run build",
    "build": "bili --moduleName VueInter --format cjs,umd,umd-min --buble.transforms.dangerousForOf",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "/"
  },
  "dependencies": {},
  "devDependencies": {
    "bili": "^1.6.5",
    "eslint-config-rem": "^3.0.0",
    "gh-pages": "^1.0.0",
    "poi": "^9.1.4",
    "tyu": "^1.0.6",
    "vue": "^2.5.6",
    "vue-template-compiler": "^2.5.6",
    "vue-test-utils": "^1.0.0-beta.5",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem/prettier",
    "envs": [
      "jest"
    ],
    "ignores": [
      "example/**"
    ],
    "rules": {
      "unicorn/filename-case": "off"
    }
  }
}
